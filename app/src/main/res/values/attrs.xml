<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FixDragLayout">
        <attr name="android:orientation" />
    </declare-styleable>

    <declare-styleable name="CustomRoundAngleImageView">
        <attr name="radius" format="dimension" />
        <attr name="left_top_radius" format="dimension" />
        <attr name="right_top_radius" format="dimension" />
        <attr name="left_bottom_radius" format="dimension" />
        <attr name="right_bottom_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CommonHeaderView">
        <attr name="chv_title" format="string" />
        <attr name="chv_back" format="boolean"/>
        <attr name="chv_text_color" format="color"/>
        <attr name="chv_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="RoundConstraintLayout">
        <attr name="round_corner" format="dimension" />
    </declare-styleable>

</resources>